{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "types": [
      "node", "jsonwebtoken",
      "multer","dotenv"
    ]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
